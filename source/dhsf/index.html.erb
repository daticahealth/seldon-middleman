                    <span data-tooltip aria-haspopup="true" class="" data-fade-in-duration="20" data-fade-out-duration="100" data-allow-html="true" data-disable-hover="false" data-v-offset="20" data-click-open="false"  title="<%= phase["desc"] %>">
                        <%= phase["label"] %>
                    </span>
                </h2>
            </div>
            <div class="columns small-12 medium-10 large-10 df-stage--dot-group">
                <ul class="tabs" data-tabs data-link-class="df-dot" data-active-collapse="true" data-deep-link="true" data-update-history="true" data-deep-link-smudge="true" data-deep-link-smudge="500" id="df-stage--timeline-<%= id %>">
                    <% phase["stages"].each_with_index do |stage, index| %>
                        <li class="df-dot" style="left: <%= ((stage["time"].to_f / 36) * 100 ).to_i %>%;">
                            <a class="df-dot--link" href="#<%= phase["slug"] %>-<%= stage["slug"] %>">
                                <span data-tooltip aria-haspopup="true" class="badge df-badge has-tip top" data-fade-in-duration="20" data-fade-out-duration="100" data-allow-html="true" data-disable-hover="false" data-v-offset="20" data-click-open="false" tabindex="<%= index %>" title="Month <%= stage["time"] %>: <%= stage["title"] %>">
                                    <%= stage["time"] %>
                                </span>
                            </a>
                        </li>
                    <% end %>
                </ul>
            </div>
        </div>
        <div class="df-stage--timeline__line"></div>

        <div class="row collapse df-series--container show-for-medium <% if id.to_i == 0 %> is-first<% end %>">
            <div class="columns small-12 medium-10 medium-offset-2 large-10 large-offset-2">
                <!-- full months list/array -->
                <ul id="df-<%= phase["slug"] %>-series" class="menu expanded df-series">
                    <li class="df-series--label">Start</li>
                    <% data.views.dhsf.timespan.each do |time| %>
                    <li class="df-series--label"><%= time %></li>
                    <% end %>
                </ul>
            </div>
        </div>

        <div id="df-<%= phase["slug"] %>-stage-content" class="tabs-content df-stage" data-animate="fade-in fade-out" data-tabs-content="df-stage--timeline-<%= id %>">
            <% phase["stages"].each_with_index do |stage, index| %>
                <div id="<%= phase["slug"] %>-<%= stage["slug"] %>" class="tabs-panel df-stage--item">
                    <div class="row">
                        <div class="columns small-12 medium-4 large-4">
                            <% if phase.has_key?("lead") %>
                                <div class="subheader-small">Strategy Overview</div>
                                <div class="lead df-phase--lead" data-columns="false">
                                    <%= Kramdown::Document.new(phase["lead"]).to_html %>
                                </div>
                            <% end %>
                        </div>
                        <div class="columns small-12 medium-8 large-8">
                            <div class="row collapse">
                                <div class="columns small-12">
                                    <div class="subheader-small text--blue">Month <%= stage["time"] %></div>
                                    <h3 class="headline-5"><%= stage["title"] %></h3>
                                </div>
                            </div>
                            <div class="df-stage--content" data-columns="<% if stage["body"].length < 400 || stage.has_key?("col_full") %>false<% else %>true<% end %>">
                                <div class="content-dynamic group"><%= Kramdown::Document.new(stage["body"]).to_html %></div>
                                <% if stage.has_key?("asides") %>
                                    <% stage["asides"].each do |aside| %>
                                        <div class="group"><%= aside %></div>
                                    <% end %>
                                <% end %>
                                <% if stage.has_key?("discovery_topic") %>
                                    <% related_discover = data.site.discover[stage.discovery_topic.to_s] %>
                                    <%= partial "/partials/cards/discover", :locals => { :post => related_discover, :classes => "group" } %>
                                <% end %>
                                <% if stage.has_key?("customer_id") %>
                                    <% related_customer = data.site.customers[stage.customer_id.to_s] %>
                                        <%= partial "/partials/content/card-case-study-compact",  :locals => { :cf_customer => related_customer, :subhead => "case study" } %>
                                <% end %>
                            </div>
                                <div class="row small-up-1 medium-up-2">
                                    <% if stage.has_key?("quote_id") %>
                                        <% related_quote = data.site.quotes[stage.quote_id.to_s] %>
                                        <!-- <blockquote class="quote-serif">
                                            <%#= Kramdown::Document.new(related_quote.quote_body).to_html %>
                                        </blockquote> -->
                                    <% end %>

                                </div>

                        </div>
                    </div>
                </div>
            <% end %>
        </div>
    </section>
<% end %>
</div>
    <section class="section-article">
