- sorted_posts = data.site.discover.to_a.sort_by{ |id, post| post['title'] }
.container-gray-10.container-image--center data-interchange="[https://images.ctfassets.net/189dvqdsjh46/6KPzYzTJLi6QgqSwAiEy06/9fb4b68a3a6f3d56449a91aa411454c9/ski_hiker_in_snow_trail__4x3_crop.jpg?w=1600&q=45, medium]" 
    = partial "partials/head/header", :locals => { :style => "dark" }
    section.section-article
        .row.align-center
            .columns.small-12.large-8.text-center.group--2x
                h1.headline-1 Discover Compliance
                p.lead= current_page.data.summary
        .row.align-center.align-middle.small-up-1.medium-up-2.large-up-3
            - sorted_posts.each do | id, post |
                .column
                    = partial "partials/cards/discover", :locals => { :post => post, :classes => "" }
    
    section.section-article
        = partial "partials/content/recent-items"