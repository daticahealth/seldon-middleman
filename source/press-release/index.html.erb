---

<section class="section-article">
    <div class="row">
        <div class="columns small-12 medium-8">
            <h2>Press Releases</h2>
            <hr class="short">
            <% data.site.pressReleases.each do |id, post| %>
                <div class="group">
                    <a href="/press-release/<%= post["slug"] %>"><%= post["title"] %></a>
                    <p>
                        <%= post["summary"] %>
                    </p>
                </div>
            <% end %>
        </div>
    </div>
</section>
